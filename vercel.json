{
  "rewrites": [
    {
      "source": "/forms/(.*)",
      "destination": "https://forms-vue-app-v2.vercel.app/$1"
    },
    {
      "source": "/cases/(.*)",
      "destination": "https://cases-vue-app.vercel.app/$1"
    },
    {
      "source": "/blogs/(.*)",
      "destination": "https://blogs-gray-omega.vercel.app/$1"
    },
    {
      "source": "/onboarding/(.*)",
      "destination": "https://onboarding-phi-virid.vercel.app/$1"
    }
  ],
  "redirects": [
    {
      "source": "/forms/(.*)",
      "destination": "https://forms-vue-app-v2.vercel.app/index.html",
      "statusCode": 200
    }
  ]
}
